{
  "editor.accessibilitySupport": "off",
  "editor.dragAndDrop": false,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.fontSize": 17,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.glyphMargin": false,
  "editor.letterSpacing": 0.5,
  "editor.lineHeight": 26,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.overviewRulerBorder": false,
  "editor.renderIndentGuides": false,
  "editor.tabSize": 2,
  "emmet.showSuggestionsAsSnippets": true,
  "eslint.nodePath":
    "/Users/xiaoyu/utilities/.nvm/versions/node/v9.7.1/bin/node",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "explorer.sortOrder": "default",
  "javascript.format.enable": false,
  "npm.packageManager": "yarn",
  "prettier.arrowParens": "always",
  "prettier.disableLanguages": ["vue", "markdown"],
  "prettier.eslintIntegration": true,
  "prettier.printWidth": 80,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.fontSize": 17,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "typescript.autoImportSuggestions.enabled": false,
  "typescript.npm": "/Users/xiaoyu/utilities/.nvm/versions/node/v9.7.1/bin/npm",
  "typescript.referencesCodeLens.enabled": false,
  "window.restoreWindows": "none",
  "window.zoomLevel": 0,
  "workbench.activityBar.visible": false,
  "workbench.editor.tabSizing": "shrink",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "newUntitledFile",
  "vim.easymotion": true,
  "vim.easymotionMarkerBackgroundColor": "#fadb14",
  "vim.easymotionMarkerFontFamily": "Fira Code",
  "vim.easymotionMarkerFontSize": "17",
  "vim.easymotionMarkerFontWeight": "500",
  "vim.easymotionMarkerForegroundColorOneChar": "#222",
  "vim.easymotionMarkerForegroundColorTwoChar": "#222",
  "vim.easymotionMarkerHeight": 22,
  "vim.easymotionMarkerWidthPerChar": 12,
  "vim.easymotionMarkerYOffset": 6,
  "vim.leader": "<space>",
  "vim.mouseSelectionGoesIntoVisualMode": false,
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "vim.otherModesKeyBindingsNonRecursive": {
    "<leader> <space>": "commands: workbench.action.quickOpen",
    "<leader> <tab>":
      "commands: workbench.action.openPreviousEditorFromHistory",
    "<leader> ; ;": "commands: editor.action.commentLine",
    "<leader> '": "commands: workbench.action.terminal.toggleTerminal",
    "<leader> /": "commands: workbench.action.findInFiles",
    "<leader> ?": "commands: workbench.action.openGlobalKeybindings",
    "<leader> [ f": "commands: editor.foldRecursively",
    "<leader> ] f": "commands: editor.unfoldRecursively",
    "<leader> [ <space>": "commands: editor.action.insertLineBefore",
    "<leader> ] <space>": "commands: editor.action.insertLineAfter",
    "<leader> 1": "commands: workbench.action.focusFirstEditorGroup",
    "<leader> 2": "commands: workbench.action.focusSecondEditorGroup",
    "<leader> 3": "commands: workbench.action.focusThirdEditorGroup",
    "<leader> b b": "commands: workbench.action.showAllEditors",
    "<leader> b d": "commands: workbench.action.closeActiveEditor",
    "<leader> b n": "commands: workbench.action.nextEditor",
    "<leader> b p": "commands: workbench.action.previousEditor",
    "<leader> b s": "commands: workbench.action.files.newUntitledFile",
    "<leader> f e": "commands: workbench.action.openGlobalSettings",
    "<leader> f f": "commands: workbench.action.files.openFileFolder",
    "<leader> f d": "commands: workbench.action.closeFolder",
    "<leader> f r": "commands: workbench.action.openRecent",
    "<leader> f s": "commands: workbench.action.files.save",
    "<leader> f t": "commands: workbench.view.explorer",
    "<leader> f y": "commands: workbench.action.files.copyPathOfActiveFile",
    "<leader> g b": "commands: git.checkout",
    "<leader> g c": "commands: git.commit",
    "<leader> g d": "commands: git.deleteBranch",
    "<leader> g f": "commands: git.fetch",
    "<leader> g i": "commands: git.init",
    "<leader> g m": "commands: git.merge",
    "<leader> g p": "commands: git.publish",
    "<leader> g s": "commands: workbench.view.scm",
    "<leader> g S": "commands: git.stage",
    "<leader> g U": "commands: git.unstage",
    "<leader> i s": "commands: editor.action.insertSnippet",
    "<leader> j =": "commands: editor.action.formatDocument",
    "<leader> j j": "after: leader leader s",
    "<leader> j l": "after: leader leader leader b d j k",
    "<leader> j w": "after: leader leader leader b d w",
    "<leader> l d": "commands: workbench.action.closeFolder",
    "<leader> p f": "commands: workbench.action.quickOpen",
    "<leader> p l": "commands: workbench.action.files.openFileFolder",
    "<leader> p p": "commands: workbench.action.openRecent",
    "<leader> p t": "commands: workbench.view.explorer",
    "<leader> q f": "commands: workbench.action.closeWindow",
    "<leader> q q": "commands: workbench.action.closeWindow",
    "<leader> q r": "commands: workbench.action.reloadWindow",
    "<leader> r s": "commands: workbench.action.findInFiles",
    "<leader> s e": "commands: editor.action.rename",
    "<leader> s j": "commands: workbench.action.gotoSymbol",
    "<leader> s p": "commands: workbench.action.findInFiles",
    "<leader> s P": "commands: workbench.action.findInFilesWithSelectedText",
    "<leader> t A": "commands: workbench.action.toggleActivityBarVisibility",
    "<leader> t M": "commands: editor.action.toggleMinimap",
    "<leader> t S": "commands: workbench.action.toggleStatusbarVisibility",
    "<leader> t W": "commands: editor.action.toggleWordWrap",
    "<leader> t F": "commands: workbench.action.toggleFullScreen",
    "<leader> t e": "commands: workbench.view.extensions",
    "<leader> t f": "commands: workbench.view.explorer",
    "<leader> t s": "commands: workbench.view.search",
    "<leader> t g": "commands: workbench.view.scm",
    "<leader> t d": "commands: workbench.view.debug",
    "<leader> t p": "commands: workbench.actions.view.problems",
    "<leader> t c": "commands: workbench.debug.action.toggleRepl",
    "<leader> t t": "commands: workbench.action.terminal.toggleTerminal",
    "<leader> t o": "commands: workbench.action.output.toggleOutput",
    "<leader> T s": "commands: workbench.action.selectTheme",
    "<leader> v": "commands: editor.action.smartSelect.grow",
    "<leader> V": "commands: editor.action.smartSelect.shrink",
    "<leader> w +": "commands: workbench.action.toggleEditorGroupLayout",
    "<leader> w =": "commands: workbench.action.evenEditorWidths",
    "<leader> w d": "commands: workbench.action.closeEditorsInGroup",
    "<leader> w h": "commands: workbench.action.focusPreviousGroup",
    "<leader> w H": "commands: workbench.action.moveEditorToPreviousGroup",
    "<leader> w j": "commands: workbench.action.joinTwoGroups",
    "<leader> w l": "commands: workbench.action.focusNextGroup",
    "<leader> w L": "commands: workbench.action.moveEditorToNextGroup",
    "<leader> w m": "commands: workbench.action.maximizeEditor",
    "<leader> w n": "commands: workbench.action.newWindow",
    "<leader> w v": "commands: workbench.action.splitEditor",
    "<leader> w w": "commands: workbench.action.focusNextGroup",
    "<leader> w W": "commands: workbench.action.focusPreviousGroup",
    "<leader> x F": "commands: editor.action.formatDocument",
    "<leader> x f": "commands: editor.action.formatSelection",
    "<leader> x s": "commands: editor.action.sortLinesAscending",
    "<leader> x S": "commands: editor.action.sortLinesDescending",
    "<leader> x w": "commands: editor.action.trimTrailingWhitespace"
  },
  "vim.insertModeKeyBindings": {
    "j j": "after: <Esc>"
  }
}
